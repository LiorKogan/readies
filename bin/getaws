#!/bin/bash

PROGNAME="${BASH_SOURCE[0]}"
HERE="$(cd "$(dirname "$PROGNAME")" &>/dev/null && pwd)"
READIES=$(cd $HERE/.. && pwd)
. $READIES/shibumi/defs

$HERE/getget
tempd=$(mktemp -d /tmp/awscli.XXXXXX)
cd $tempd
runn curl -q "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"
runn unzip -q awscliv2.zip
runn $SUDO ./aws/install --update
cd $HERE
rm -rf $tempd

runn python3 -m pip install s3cmd
